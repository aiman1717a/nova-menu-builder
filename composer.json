{
    "name": "aiman/nova-menu-builder",
    "description": "Advanced Menu Builder Tool for Laravel Nova.",
    "keywords": [
        "laravel",
        "nova",
        "menu",
        "builder",
        "Aiman",
        "Thaana"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Aiman",
            "email": "aiman1717a@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "spatie/laravel-translatable": ">=3.1"
    },
    "autoload": {
        "psr-4": {
            "Aiman\\MenuBuilder\\": "src/"
        },
        "files": [
          "src/Http/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aiman\\MenuBuilder\\MenuBuilderServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
